            "justMyCode": true
        },
        {
            "name": "allrank_train",
            "type": "python",
            "request": "launch",
            "program": "allrank/main.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": [
                "--config-file-name",
                "allrank/neuralndcg_atmax_Fold1_normalized.json",
